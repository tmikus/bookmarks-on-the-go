{
  "manifest_version": 2,
  "name": "Bookmarks on the go",
  "description": "Insert some inspirational description...",
  "version": "1.0.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Bookmarks on the go",
    "default_popup": "popup.html"
  }
}
